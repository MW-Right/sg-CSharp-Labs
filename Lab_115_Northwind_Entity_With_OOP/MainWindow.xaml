<Window x:Class="Lab_115_Northwind_Entity_With_OOP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_115_Northwind_Entity_With_OOP"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="Active" x:FieldModifier="public" HorizontalAlignment="Left" Height="220" Margin="519,144,0,0" VerticalAlignment="Top" Width="208" SelectionChanged="Active_SelectionChanged"/>
        <ListBox x:Name="Inactive" HorizontalAlignment="Left" Height="220" Margin="72,144,0,0" VerticalAlignment="Top" Width="194" SelectionChanged="Inactive_SelectionChanged"/>
        <RadioButton x:Name="makeActive" Content="Make Active" HorizontalAlignment="Left" Margin="344,94,0,0" VerticalAlignment="Top" Checked="MakeActive_Checked"/>
        <TextBlock HorizontalAlignment="Left" Margin="72,123,0,0" TextWrapping="Wrap" Text="InActive" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="519,123,0,0" TextWrapping="Wrap" Text="Active" VerticalAlignment="Top"/>
    </Grid>
</Window>
